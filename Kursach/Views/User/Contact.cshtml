@{
    ViewData["Title"] = "Contact Form";
    Layout = "~/Views/Shared/User.cshtml";
}
<div class="text-center">
    <h1 class="display-4">Contact Form</h1>
    <a>if you have questions you can call administrator:</a>
</div>
   
    <table class="styled-table2">

        <tr>
            <th>Surname</th>
            <td>@ViewBag.User.Surname</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@ViewBag.User.Name</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@ViewBag.User.Email</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>@ViewBag.User.Phone</td>
        </tr>



    </table>
